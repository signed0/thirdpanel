<!doctype html>
<html lang="en">
    <head>
        <title>3rd Panel</title>
        <link href="http://twitter.github.com/bootstrap/assets/css/bootstrap.css" rel="stylesheet">
        <style>
        body {
            background-color: #e9e9e9;
        }        
        </style>
    </head>
    <body>
        <div class="container">
            <h1>3rd Panel</h1>

            <ul class="list">
                {% for feed in feeds|sort %}
                <li><a href="{{ feed }}">{{ feed }}</a></li>
                {% endfor %}
            </ul>

        </div>
    </body>
</html>